<%- if items.present? -%>
<div class="carousel-wrap">
	<div class='carousel slide' id='gallery_images'>
	  <ul class="carousel-inner unstyled">
	    <%= render items %>
	  </ul>
	</div>
	<div class="detail-wrap">
		<div class="details">
			<h2>gallery.title</h2>
			raw gallery.description 
			<div>
			<%= raw galleries %>
			</div>
			<div><%= raw items %></div>
		</div>
	</div>
</div>
<ul class="unstyled">
	<li class="thumbnails">
		<a href="#" class="icon-thumbs" title="View Info">View thumbnails</a>
	</li>
	<li class="project-title">
		<h6>title</h6>
	</li>
	<li class="info">
		<a href="#" class="icon-info" title="View Project Info">View project info</a>
	</li>
</ul>
<%- end -%>
